<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>ap1.servlets.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet3</servlet-name>
        <servlet-class>ap1.servlets.NewServlet3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logowanie</servlet-name>
        <servlet-class>ap1.servlets.Logowanie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostLogin</servlet-name>
        <servlet-class>ap1.servlets.PostLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Kalkulator</servlet-name>
        <servlet-class>ap1.servlets.Kalkulator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParametersServlet</servlet-name>
        <servlet-class>ap1.servlets.ParametersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReciveParameters</servlet-name>
        <servlet-class>ap1.servlets.ReciveParameters</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Recive</servlet-name>
        <servlet-class>ap1.servlets.Recive</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet3</servlet-name>
        <url-pattern>/NewServlet3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logowanie</servlet-name>
        <url-pattern>/Logowanie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostLogin</servlet-name>
        <url-pattern>/PostLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Kalkulator</servlet-name>
        <url-pattern>/Kalkulator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParametersServlet</servlet-name>
        <url-pattern>/ParametersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReciveParameters</servlet-name>
        <url-pattern>/ReciveParameters</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Recive</servlet-name>
        <url-pattern>/Recive</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
